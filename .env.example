# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# App URL
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Stripe (currently disabled)
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret
STRIPE_PRO_PRICE_ID=your_stripe_pro_price_id
STRIPE_BUSINESS_PRICE_ID=your_stripe_business_price_id

# AI Provider API Keys (optional - users can also configure in settings)
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
GOOGLE_AI_API_KEY=your_google_ai_api_key
OPENROUTER_API_KEY=your_openrouter_api_key
REQUESTY_API_KEY=your_requesty_api_key
GEMINI_API_KEY=your_gemini_api_key

# GitHub Integration
GITHUB_TOKEN=your_github_personal_access_token

# Bright Data (Optional - for web scraping)
BRIGHT_DATA_USERNAME=your_bright_data_username
BRIGHT_DATA_PASSWORD=your_bright_data_password

# Google Cloud (Optional - for advanced document processing)
GOOGLE_CLOUD_PROJECT_ID=your_project_id
GOOGLE_CLOUD_LOCATION=your_location
GOOGLE_CLOUD_PROCESSOR_ID=your_processor_id
GOOGLE_APPLICATION_CREDENTIALS=path_to_credentials.json

# Development SSL Configuration
# If you encounter SSL certificate errors in development, you can:
# 1. Use 'npm run dev' which includes NODE_TLS_REJECT_UNAUTHORIZED=0
# 2. Or set this in your .env.local file (NOT RECOMMENDED for production):
# NODE_TLS_REJECT_UNAUTHORIZED=0